# focus between windows
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east
alt - k : yabai -m window --focus north
alt - j : yabai -m window --focus south

# focus space
alt - 1 : yabai -m space --focus term
alt - 2 : yabai -m space --focus browser
alt - 3 : yabai -m space --focus mail

# move windows
lshift + alt - k : yabai -m window --swap north
lshift + alt - j : yabai -m window --swap south
lshift + alt - l : yabai -m window --swap east
lshift + alt - h : yabai -m window --swap west

# delete space
alt - d : yabai -m space --destroy

# zoom
alt - z : yabai -m window --toggle zoom-fullscreen

# rotate
alt - r : yabai -m space --rotate 90
