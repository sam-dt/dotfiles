# === General === #
alias ll='ls -l'
alias lla='ls -lA'
alias uuidgen_lower="uuidgen |tr '[:upper:]' '[:lower:]'"

# === Git === #
alias gl='git pull'
alias gco='git checkout'
alias gp='git push'
alias gpF='git push --force'
alias gm='git merge'
alias gmt='git mergetool --no-prompt'
alias gss='git status -s'
alias gc='git commit -v'
alias gaa='git add .'
alias gaac='git add . && git commit -m'
alias gst='git stash push'
alias gsta='git stash apply'
alias gstm='git stash push -m'
alias gstl='git stash list'
alias gpsup='git push --set-upstream origin "$(git_current_branch)"'
alias gaac='git add . && git commit -m'
alias gdn='git diff --name-only'
alias glg='git log --graph'
alias glgp='git log --graph -p'
alias gslg='git shortlog'
alias glgo='git log --oneline'
alias glgos='git log --oneline -5 |cat'
alias glnf='git pull --no-ff'
alias gd='git diff'
alias gdd='git diff development.."$(git_current_branch)"'
alias gdm='git diff master.."$(git_current_branch)"'
alias gfa='git fetch --all --prune'
alias gr='git reset'
alias grh='git reset HEAD'
alias grhh='git reset HEAD --hard'
alias gcb='git checkout -b'
alias gb='git branch'
alias gba='git branch -a'
alias gbd='git branch -d'
alias gbD='git branch -D'
alias grb='git rebase -i'
alias grbc='git rebase --continue'

# === Docker === #
alias du='docker start'
alias dd='docker stop'
alias dr='docker restart'
alias dlgf='docker logs --follow'
alias dlg='docker logs'
alias drm='docker rm'
alias drmi='docker rmi'
alias dps='docker ps'
alias db='docker build'

# === Tmux === #
alias tn='tmux new-session'
alias tns='tmux new-session -s'
alias tk='tmux kill-session -t'
alias tka='tmux kill-session -a'
alias ta='tmux attach-session'
alias tas='tmux attach-session -t'
alias tls='tmux ls'

# === Scripts === #
alias pbastart='~/scripts/startupPBA.sh'
alias pbalogin='~/scripts/loginPBA.sh'
alias pbactx='~/scripts/startContextPBA.sh'

# === Application === #
alias chrome='open -a /Applications/Google\ Chrome.app'
alias firefox='open -a /Applications/Firefox.app'

# === Edit configurations === #
alias ezsh='nvim ~/.dotfiles/.zshrc && source ~/.zshrc'
alias eali='nvim ~/.dotfiles/.alias && source ~/.alias'
alias efun='nvim ~/.dotfiles/.function && source ~/.function'
alias egit='nvim ~/.dotfiles/.gitconfig && source ~/.gitconfig'
alias etmu='nvim ~/.dotfiles/.tmux.conf && tmux source-file ~/.tmux.conf'
alias eenv='nvim ~/.dotfiles/.env && source ~/.env'

# === Projects === #
# PBA
alias generateTypes='yarn generate:types --header "Authorization: Bearer $(pbalogin)"'
